{
	"manifest_version": 2,
	"name": "window.opener vulnerability detector",
	"version": "0.1",

	"description": "Checks for window.opener vulnerabilities as you browse.",

	"permissions": ["storage"],

	"options_ui": {
		"page": "options.html"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": [
				"ignore_list.js",
				"check.js"
			]
		}
	],

	"web_accessible_resources": [
		"vuln_report_page.html",
		"vuln_report_page.js"
	]
}
