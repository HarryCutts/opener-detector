{
	"manifest_version": 2,
	"name": "window.opener vulnerability detector",
	"version": "0.1",

	"description": "Checks for window.opener vulnerabilities as you browse.",

	"permissions": ["storage"],

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["check.js"]
		}
	],

	"web_accessible_resources": [
		"vuln_report_page.html",
		"vuln_report_page.js"
	]
}
